{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

    <div class="main" role="main">
        <div class="wrapper">
            <div class="grid">
                <div class="side-analysis">
                    <h3>Meny</h3>
                    {% for page in pages("analysis")|sort_by("time") %}
                    <div class="post">
                        <h3><a class="grow" href="{{ page.url }}">{{ page.title }}</a></h3>
                        <p class="excerpt">{{ page.description }}</p>
                    </div>
                    {% endfor %}
                </div>
                <div class="content">
                    {{ content }}
                </div>
            </div>
        </div>
    </div>
    
    {% include 'incl/footer.twig' %}
</body>

</html>
